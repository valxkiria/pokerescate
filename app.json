{
  "expo": {
    "name": "pokerescate",
    "slug": "pokerescate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFABAB"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFABAB"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.valxkiria.pokerescate"
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "b7c094c3-ff44-44ec-97b6-e98abd09f06c"
      }
    }
  }
}
