{
  "expo": {
    "name": "pokerescate",
    "slug": "pokerescate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFABAB"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFABAB"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ]
  }
}
